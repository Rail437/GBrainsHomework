      <TextField fx:id="loginInput" layoutX="118.0" layoutY="138.0" prefHeight="26.0" prefWidth="214.0" promptText="Enter login" />
      <TextField fx:id="passwordInput" layoutX="117.0" layoutY="187.0" prefHeight="26.0" prefWidth="214.0" promptText="Enter password" />
   </children>
</AnchorPane>
